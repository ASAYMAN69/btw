{
  "name": "browse-the-web",
  "version": "2.0.2",
  "description": "AI Browser Automation API - Control Headless Chrome via RESTful HTTP endpoints. Perfect for web scraping, RPA, automated testing, and AI agent integration with 70+ endpoints including screenshots, PDF generation, network monitoring, and more.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:old": "node app.js",
    "dev": "ts-node src/index.ts",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "test": "node dist/test/index.js",
    "test:basic": "curl -s http://localhost:5409/api/health && echo",
    "clean": "rm -rf dist",
    "all": "npm run clean && npm run build && npm start",
    "lint": "echo 'Linting not configured yet'",
    "typecheck": "npx tsc --noEmit"
  },
